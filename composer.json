{
  "name": "50sq/commerce-support",
  "description": "Commerce Support by 50Squared",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "commerce",
    "cart",
    "50sq",
    "50squared"
  ],
  "authors": [
    {
      "name": "Adam Campbell",
      "email": "adam@50sq.com"
    },
    {
      "name": "Yaz Jallad",
      "email": "yaz@50sq.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": "^7.4|^8.0",
    "ext-json": "*",
    "gmostafa/php-graphql-client": "^1.10",
    "illuminate/database": "^7.0|^8.0|^9.0",
    "illuminate/support": "^7.0|^8.0|^9.0",
    "printful/php-api-sdk": "^2.2"
  },
  "require-dev": {
    "fakerphp/faker": "^1.9",
    "hotmeteor/eco-env": "^1.1",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^4.0|^5.4",
    "orchestra/database": "^4.0|^5.0|^6.0",
    "orchestra/testbench": "^4.0|^5.0|^6.0",
    "phpunit/phpunit": "^8.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "FiftySq\\Commerce\\Support\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  }
}
